<?xml version='1.0' encoding='UTF-8'?>
<hudson>
  <disabledAdministrativeMonitors/>
  <version>1.0</version>
  <numExecutors>2</numExecutors>
  <mode>NORMAL</mode>
  <useSecurity>{{ use_security }}</useSecurity>
  <authorizationStrategy class="hudson.security.GlobalMatrixAuthorizationStrategy">
    <permission>com.cloudbees.plugins.credentials.CredentialsProvider.Create:{{ allowed_user }}</permission>
    <permission>com.cloudbees.plugins.credentials.CredentialsProvider.Delete:{{ allowed_user }}</permission>
    <permission>com.cloudbees.plugins.credentials.CredentialsProvider.ManageDomains:{{ allowed_user }}</permission>
    <permission>com.cloudbees.plugins.credentials.CredentialsProvider.Update:{{ allowed_user }}</permission>
    <permission>com.cloudbees.plugins.credentials.CredentialsProvider.View:{{ allowed_user }}</permission>
    <permission>hudson.model.Computer.Build:{{ allowed_user }}</permission>
    <permission>hudson.model.Computer.Configure:{{ allowed_user }}</permission>
    <permission>hudson.model.Computer.Connect:{{ allowed_user }}</permission>
    <permission>hudson.model.Computer.Create:{{ allowed_user }}</permission>
    <permission>hudson.model.Computer.Delete:{{ allowed_user }}</permission>
    <permission>hudson.model.Computer.Disconnect:{{ allowed_user }}</permission>
    <permission>hudson.model.Hudson.Administer:{{ allowed_user }}</permission>
    <permission>hudson.model.Hudson.ConfigureUpdateCenter:{{ allowed_user }}</permission>
    <permission>hudson.model.Hudson.Read:{{ allowed_user }}</permission>
    <permission>hudson.model.Hudson.RunScripts:{{ allowed_user }}</permission>
    <permission>hudson.model.Hudson.UploadPlugins:{{ allowed_user }}</permission>
    <permission>hudson.model.Item.Build:{{ allowed_user }}</permission>
    <permission>hudson.model.Item.Cancel:{{ allowed_user }}</permission>
    <permission>hudson.model.Item.Configure:{{ allowed_user }}</permission>
    <permission>hudson.model.Item.Create:{{ allowed_user }}</permission>
    <permission>hudson.model.Item.Delete:{{ allowed_user }}</permission>
    <permission>hudson.model.Item.Discover:{{ allowed_user }}</permission>
    <permission>hudson.model.Item.Read:{{ allowed_user }}</permission>
    <permission>hudson.model.Item.Workspace:{{ allowed_user }}</permission>
    <permission>hudson.model.Run.Delete:{{ allowed_user }}</permission>
    <permission>hudson.model.Run.Update:{{ allowed_user }}</permission>
    <permission>hudson.model.View.Configure:{{ allowed_user }}</permission>
    <permission>hudson.model.View.Create:{{ allowed_user }}</permission>
    <permission>hudson.model.View.Delete:{{ allowed_user }}</permission>
    <permission>hudson.model.View.Read:{{ allowed_user }}</permission>
    <permission>hudson.scm.SCM.Tag:{{ allowed_user }}</permission>
  </authorizationStrategy>
  <securityRealm class="hudson.security.HudsonPrivateSecurityRealm">
    <disableSignup>{{ disable_signup }}</disableSignup>
    <enableCaptcha>false</enableCaptcha>
  </securityRealm>
  <disableRememberMe>false</disableRememberMe>
  <projectNamingStrategy class="jenkins.model.ProjectNamingStrategy$DefaultProjectNamingStrategy"/>
  <workspaceDir>${JENKINS_HOME}/workspace/${ITEM_FULLNAME}</workspaceDir>
  <buildsDir>${ITEM_ROOTDIR}/builds</buildsDir>
  <markupFormatter class="hudson.markup.EscapedMarkupFormatter"/>
  <jdks/>
  <viewsTabBar class="hudson.views.DefaultViewsTabBar"/>
  <myViewsTabBar class="hudson.views.DefaultMyViewsTabBar"/>
  <clouds/>
  <scmCheckoutRetryCount>0</scmCheckoutRetryCount>
  <views>
    <hudson.model.AllView>
      <owner class="hudson" reference="../../.."/>
      <name>All</name>
      <filterExecutors>false</filterExecutors>
      <filterQueue>false</filterQueue>
      <properties class="hudson.model.View$PropertyList"/>
    </hudson.model.AllView>
  </views>
  <primaryView>All</primaryView>
  <slaveAgentPort>0</slaveAgentPort>
  <label></label>
  <nodeProperties/>
  <globalNodeProperties/>
</hudson>
